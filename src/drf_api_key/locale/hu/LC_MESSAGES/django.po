#: src/drf_api_key/admin.py:46
#, python-brace-format
msgctxt "messages"
msgid ""
"The API key for {name} is: {key}. Please store it somewhere safe: you will "
"not be able to see it again."
msgstr ""
"Az API kulcshoz ({name}) tartozó érték: {key} \n"
"Tárolja biztonságos helyen, a kulcs értéke csak egyszer jelenik meg."

#: src/drf_api_key/models.py:51
msgctxt "errors"
msgid "Key is not valid."
msgstr "A kulcs érvénytelen."

#: src/drf_api_key/models.py:79
msgctxt "fields"
msgid "id"
msgstr "azonosító"

#: src/drf_api_key/models.py:81
msgctxt "fields"
msgid "prefix"
msgstr "előtag"

#: src/drf_api_key/models.py:82
msgctxt "fields"
msgid "hashed key"
msgstr "kulcs hash"

#: src/drf_api_key/models.py:83
msgctxt "fields"
msgid "created"
msgstr "létrehozva"

#: src/drf_api_key/models.py:85
msgctxt "fields"
msgid "name"
msgstr "név"

#: src/drf_api_key/models.py:89
msgctxt "helptext"
msgid "A free-form unique name for the API key."
msgstr "Szabadon választható egyedi elnevezés."

#: src/drf_api_key/models.py:92
msgctxt "fields"
msgid "revoked"
msgstr "visszavonva"

#: src/drf_api_key/models.py:95
msgctxt "helptext"
msgid "If the API key is revoked, clients cannot use it anymore."
msgstr "A visszavont apikulcsot nem lehet többé használni."

#: src/drf_api_key/models.py:98
msgctxt "fields"
msgid "expiry date"
msgstr "lejárati dátum"

#: src/drf_api_key/models.py:101
